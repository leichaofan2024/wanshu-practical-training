
<div class="root-search-frame">
  <div class="root-search">
    <b>搜索：</b>
    <div style="display:inline-block;">
      <%= form_for t_station_infoes_path do |f| %>
        <%= f.text_field  :date,:placeholder => "输入搜索内容", :style => "height:25px;font-size:10px;"%>
      <% end %>
    </div>

  </div>

  <div class="root-search-time1">
    <b>时间：</b>
    <div style="display:inline-block;">
      <%= form_for t_station_infoes_path do |f| %>
        <%= f.text_field  :date, :placeholder => "开始日期...",:style => "height:25px;font-size:10px;"%>
      <% end %>
    </div>

  </div>
  ~
  <div class="root-search-time2">
    <%= form_for t_station_infoes_path do |f| %>
      <%= f.text_field  :date, :placeholder => "截止日期...",:style => "height:25px;font-size:10px;"%>
    <% end %>
  </div>
  <button type="button" name="button">确定</button>
</div>
<div class="root-card-frame">
  <div class="root-card">
    <div class="root-card-block">
      <strong>站段总数：<%= TDuanInfo.all.count - 2 %> </strong><br>
      <strong>实考站段：<%= duan_ck.count %></strong>
    </div>
    <div class="root-card-block">
      <strong>车站总数：<%= station.count%> </strong><br>
      <strong>实考车站：<%= station_ck.count %></strong>
    </div>
    <div class="root-card-block">
      <strong>班组总数：<%= team.count %> </strong><br>
      <strong>实考班组：<%= team_ck.count %></strong>
    </div>
    <div class="root-card-block">
      <strong>实训人数：<%= student.count %> </strong><br>
      <strong>实考人数：<%= student_ck.count %></strong>
    </div>
    <div class="root-card-block">
      <strong>考点总数：<%= TProgramInfo.all.count %> </strong><br>
      <strong>覆盖考点：<%= program_ck.count %></strong>
    </div>
  </div>

</div>

<div class="root-data-frame">
  <div class="root-data-block">
    <div class="root-data-people">
      <%= student_ck.count.to_f/student.count %>
    </div>
    <div class="root-data-score">
      <%= %>
    </div>
    <div class="root-data-question">

    </div>
    <div class="root-data-wrong">

    </div>
  </div>

</div>
